{
    "name": "lucascavalcante/fuzzy-search",
    "description": "A PHP library that turns easy fuzzy string searching",
    "type": "library",
    "license": "MIT",
    "keywords": ["fuzzy", "matching", "search", "searching", "string"],
    "authors": [
        {
            "name": "Lucas Cavalcante",
            "email": "lucascavalcante@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.0",
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": {
            "FuzzySearch\\": "src/FuzzySearch"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.99"
    }
}
